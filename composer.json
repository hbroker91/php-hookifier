{
    "name": "Hbroker/php-hookifier",
    "description": "Package to add lifecycle hook(s) to any class easily",
    "keywords": [
        "lifecycle",
        "hooks",
        "php"
    ],
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Szalmasági Ádám",
            "email": "adam.szalmasagi@gmail.com",
            "role": "Developer"
        }
    ],
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "Hbroker\\PHPHookifier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hbroker\\PHPHookifier\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "8"
    }
}
